{
    "$schema": "https://json.schemastore.org/prettierrc",
    "plugins": ["@prettier/plugin-pug"],
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "pugPrintWidth": 140,
    "trailingComma": "none",
    "bracketSpacing": true,
    "tabWidth": 4,
    "useTabs": false,
    "quoteProps": "as-needed",
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSameLine": true
}
